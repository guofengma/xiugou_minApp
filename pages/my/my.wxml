<!--pages/my/account.wxml-->
<view class="order sub-page">
    <view class="bg-area" bindtap="personalData">
        <view class="img-area">
            <image class="avatar-img" src="{{userInfos.headImg||'/img/default-userImg.png'}}" alt=""></image>
        </view>
        <view class="info">
            <view>
                <text>{{userInfos.nickname}}</text>
                <image class="smrz-img" wx:if='{{userInfos.realnameStatus == 1}}' src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/smyh_03@2x.png" alt=""></image>
            </view>
            <view>{{userInfos.levelName || ''}}</view>
        </view>
        <view>
            <image class="jt-img" src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/jt_06@2x.png" alt=""></image>
        </view>
    </view>
    <view class="my-order">
        <view class="title" bindtap="itemClicked" data-query="0" data-page="3">
            <view><text class='flexR'>我的订单</text></view>
            <view>
                <text>全部订单</text>
                <image class="jt-img" src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/xjt_10@2x.png" alt=""></image>
            </view>
        </view>
        <view class="clearfix"></view>
        <view class="body">
            <view bindtap="itemClicked" data-query="1" data-page="3">
                <image src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/dfk_03@2x.png" alt=""></image>
                <view>待付款</view>
            </view>
            <view bindtap="itemClicked" data-query="2" data-page="3">
                <image src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/wfh_03-02@2x.png" alt=""></image>
                <view>待发货</view>
            </view>
            <view bindtap="itemClicked" data-query="3" data-page="3">
                <image src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/yfh_03-03@2x.png" alt=""></image>
                <view>待收货</view>
            </view>
            <view bindtap="itemClicked" data-page="4">
                <image src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/ywc_03-05@2x.png" alt=""></image>
                <view>售后/退款</view>
            </view>
        </view>
        
          <view class='my-rank'>
             <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconRank.png' mode='widthFix'></image>
             <text class='my-rank-no'>V1</text>
          </view>
       </view>
   </view>
   <!--  -->
   <view class='my-save'>
      <view>21.00元</view>
      <view>已帮你省</view>
   </view>
  </view>
  <!-- 我的资产 -->
  <view class='my-account'>
    <view class='my-account-item'>我的资产</view>
    <view class='my-account-list'>
       <view class='my-account-coin'>
         <view class='account-num'>3639.123</view>
         <view>现金账户</view>
       </view>
        <view class='my-account-xiudou'>
         <view class='account-num'>3639.123</view>
         <view>秀逗账户</view>
       </view>
        <view class='my-account-leave'>
         <view class='account-num'>3639.123</view>
         <view>待提取账户</view>
       </view>
    </view>
    <view class="wrap">
      <view class="item line" bindtap="itemClicked" data-page="0" data-query='{{userInfos.blockedBalance}}'>
            <view>
                <image src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/wdzh_18@2x.png" alt=""></image>
                <text>待提现账户{{userInfos.blockedBalance}}</text>
            </view>
            <view>
                <!-- <text>更多</text> -->
                <image class="jt-img" src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/xjt_10@2x.png' mode='widthFix'></image>
            </view>
        </view>
        <view class="item line" bindtap="itemClicked" data-page="1" data-query='{{userInfos.userScore}}'>
            <view>
                <image src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/coupon.png" alt=""></image>
                <text>秀豆账户{{userInfos.userScore}}</text>
            </view>
            <view>
                <image class="jt-img" src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/xjt_10@2x.png' mode='widthFix'></image>
            </view>
        </view>

        <view class="item line" bindtap="itemClicked" data-page="2" data-query='{{userInfos.availableBalance}}'>
            <view>
                <image src="https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/wdzh_18@2x.png" alt=""></image>
                <text>余额账户{{userInfos.availableBalance}}</text>
            </view>
            <view>
                <!-- <text>更多</text> -->
                <image class="jt-img" src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/xjt_10@2x.png' mode='widthFix'></image>
            </view>
        </view>
        <view class='my-tools-cont'>
          <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconCalendar.png' mode='widthFix' class='my-calendar'></image>
          <view>活动日历</view>
        </view>
        <view class='my-tools-cont'>
          <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconVoucher.png' mode='widthFix' class='my-voucher'></image>
          <view>优惠券</view>
        </view>
        <view class='my-tools-cont'>
          <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconData.png' mode='widthFix' class='my-data'></image>
          <view>我的数据</view>
        </view>
        <view class='my-tools-cont'>
          <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconCollect.png' mode='widthFix' class='my-collect'></image>
          <view>收藏店铺</view>
        </view>
        <view class='my-tools-cont'>
          <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconHelp.png' mode='widthFix' class='my-help'></image>
          <view>帮助</view>
        </view>
        <view class='my-tools-cont'>
          <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconAddress.png' mode='widthFix' class='my-address'></image>
          <view>地址</view>
        </view>
        <view class='my-tools-cont'>
          <image src='https://dnlcrm.oss-cn-beijing.aliyuncs.com/xcx/iconTrack.png' mode='widthFix' class='my-track'></image>
          <view>足迹</view>
        </view>
     </view>
     
  </view>
</view>

