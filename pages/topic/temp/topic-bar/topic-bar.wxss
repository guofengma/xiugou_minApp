.tab-h{
  height: 96rpx;
  width: 100%; 
  overflow: hidden;
  line-height: 96rpx;
  white-space: nowrap;
  /* margin-bottom: 20rpx; */
}
/* .tab-h-view{
  padding: 0 30rpx;
} */
.tab-item{
  width:50%;
  display: inline-block;
  text-align: center;
  position: relative;
  font-size: 32rpx;
}
.tab-item text{
  display: inline-block;
  width: 100%;
  height: 26rpx;
  line-height: 26rpx;
  border-right: 1rpx solid #eee;
  padding: 5rpx 0;
}
.tab-h .tab-item:last-child text{
  border-right: 1rpx solid transparent;
}