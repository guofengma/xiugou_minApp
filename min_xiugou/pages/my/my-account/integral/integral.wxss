.cash{
    padding: 20rpx 30rpx;
}
.item{
    width: 100%;
    height: 190rpx;
    color: #fff;
    margin-bottom: 20rpx;
    /* background: url("mrdomain/icon11_03@2x.png") no-repeat top center;
    background-size:100% 100%; */
    background: #F2D4A2;
    border-radius: 16rpx;
    padding: 30rpx;
    align-items: flex-start;
}
.num{
    font-size: 48rpx;
}
.btn{
    width: 200rpx;
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 10rpx;
    border: solid 1rpx #ffffff;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    position: absolute;
    bottom:75rpx;
    right: 50rpx;
}
.detail-content{
  width: 70%;
  justify-content: flex-start
}
.detail{
    /* width: 48%; */
    margin-top: 15rpx;
}
.list-item{
    display: flex;
    justify-content: space-around;
    background: #fff;
    width: 100%;
    margin-bottom: 2rpx;
    padding: 30rpx 0;
}
.list-item image{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-top: 10rpx;
    margin-right: 30rpx;
}
.style{
    font-size: 30rpx;
    color: #222222;
}
.money{
    font-size: 36rpx;
    margin-top: 15rpx;
}
.time{
    margin: 10rpx 0;
}
.time,.number{
    font-size: 26rpx;
    color: #999999;
}
.coinBtn{
  height: 88rpx;
  width: 210rpx;
  color: #fff;
  border:1rpx solid;
  border-radius: 10rpx;
}
