@import "/css/app";
.content{
    width: 80%;
    min-height: 720rpx;
    background: #fff;
    border-radius: 10rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    padding-bottom: 20rpx;
}
.header{
    background: url("${mrdomain}/dhk_07@2x.png") no-repeat top center;
    background-size: cover;
    width: 100%;
    height: 165rpx;
    line-height: 165rpx;
    font-size: 36rpx;
    color: #ffffff;
    text-align: center;
}
.info{
    padding: 40rpx 40rpx 0;
}
.close{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    top:-20rpx;
    right:-40rpx;
}